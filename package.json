{
  "name": "eslint-rules",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "rsbuild dev --open",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,html,css,scss}",
    "lint": "eslint . --fix",
    "pretty": "yarn format && yarn lint"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.0.1",
    "@rsbuild/plugin-react": "^1.0.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.5.2"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{ts}": [
      "prettier --write",
      "eslint --no-error-on-unmatched-pattern --fix"
    ]
  }
}
